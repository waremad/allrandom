import random

def zoro(self):
    c = self[0]
    n = 0
    while self[n] == c:
        n += 1
    return n

one = []

for i in range(10**8):
    n = str(random.random())[2:]
    while len(one)-1 < int(zoro(n)):
        one.append([0]*10)
    one[int(zoro(n))][int(n[0])] += 1
for i in range(len(one)):
    print(i,one[i])

